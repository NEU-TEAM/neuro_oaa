<?xml version="1.0"?>

<launch>

  <!-- Initialize robot arm and hand -->
  

  <!-- Initialize vision system on host machine -->
  <include file="$(find drv_brain)/launch/drv_v2_host.launch">
    <arg name="vision_namespace" value="vision"/>

    <!-- Referance frame of the vision system -->
    <arg name="base_link_frame" value="base_link"/>

    <arg name="recog_method" value="0"/>
  </include>

</launch>
